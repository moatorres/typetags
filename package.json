{
  "name": "typetags",
  "version": "1.0.0",
  "decription": "Node object toString type tags",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "author": "Moa Torres",
  "license": "ISC",
  "scripts": {
    "build": "yarn clean && rollup -c && yarn build:types",
    "build:dev": "rollup -c -w",
    "build:types": "copyfiles -u 1 \"lib/*.d.ts\" dist/types",
    "release": "yarn build && cp README.md package.json dist/ && cd dist && yarn publish --access public",
    "test": "jest",
    "test:dev": "jest --watchAll --no-cache",
    "test:clear": "jest --clearCache",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist/"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "setupFilesAfterEnv": "jest-extended"
  },
  "keywords": [
    "type",
    "tags",
    "typetags",
    "node",
    "object",
    "tag",
    "toString",
    "node"
  ],
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/jest": "^26.0.24",
    "copyfiles": "^2.4.1",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.1",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.5"
  }
}
